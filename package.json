{
    "name": "fibook",
    "version": "0.0.1",
    "description": "Book manager",
    "main": "server.js",
    "repository": "https://github.com/isdenmois/fibook.git",
    "author": "Denis Moiseev <isdenmois@gmail.com>",
    "license": "MIT",
    "scripts": {
        "analyze:clean": "rimraf stats.json",
        "preanalyze": "npm run analyze:clean",
        "analyze": "node ./scripts/analyze.js",
        "postinstall": "npm run build:dll",
        "prebuild": "npm run build:clean",
        "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --color --progress",
        "build:details": "npm run build -- --display-error-details",
        "build:clean": "rimraf ./build",
        "build:dll": "node ./webpack/dependencies.js",
        "postbuild": "npm run compress",
        "compress": "node ./scripts/gzip.js",
        "start": "cross-env NODE_ENV=development node server",
        "start:prod": "cross-env NODE_ENV=production node server",
        "lint": "eslint src",
        "test": "jest"
    },
    "dependencies": {
        "atob": "^2.0.3",
        "immutable": "^3.8.1",
        "lodash": "^4.17.3",
        "material-ui": "^0.16.5",
        "normalizr": "^2.3.1",
        "querystring": "^0.2.0",
        "react": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-redux": "^5.0.1",
        "react-router": "^3.0.0",
        "react-router-redux": "^2.1.0",
        "react-tap-event-plugin": "^2.0.1",
        "redbox-react": "^1.3.3",
        "redux": "^3.6.0",
        "redux-logger": "^2.7.4",
        "redux-saga": "^0.13.0",
        "redux-thunk": "^2.1.0",
        "reselect": "^2.5.4",
        "translit": "^0.0.1",
        "translit-russian": "^0.0.2",
        "xml-query": "^1.4.0",
        "xml-reader": "^2.4.3"
    },
    "devDependencies": {
        "add-asset-html-webpack-plugin": "^1.0.2",
        "babel-core": "^6.21.0",
        "babel-eslint": "^7.1.1",
        "babel-jest": "^18.0.0",
        "babel-loader": "^6.2.10",
        "babel-plugin-uglify": "^1.0.2",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.16.0",
        "babili-webpack-plugin": "^0.0.7",
        "body-parser": "^1.15.2",
        "connect-multiparty": "^2.0.0",
        "cross-env": "^3.1.3",
        "css-loader": "^0.26.1",
        "ejs-loader": "^0.3.0",
        "enzyme": "^2.7.0",
        "eslint": "^3.12.2",
        "eslint-config-airbnb": "^13.0.0",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^2.2.3",
        "eslint-plugin-react": "^6.8.0",
        "exports-loader": "^0.6.3",
        "express": "^4.14.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^2.24.1",
        "identity-obj-proxy": "^3.0.0",
        "imports-loader": "^0.7.0",
        "jest": "^18.1.0",
        "json-loader": "^0.5.4",
        "md5-file": "^3.1.1",
        "morgan": "^1.7.0",
        "node-sass": "^4.1.1",
        "node-zopfli": "^2.0.2",
        "react-addons-test-utils": "^15.4.1",
        "rimraf": "^2.5.4",
        "sass-loader": "^4.1.1",
        "shelljs": "^0.7.5",
        "sqlite3": "^3.1.8",
        "strip-loader": "^0.1.2",
        "style-loader": "^0.13.1",
        "uglify-js": "mishoo/UglifyJS2#harmony",
        "webpack": "^1.14.0",
        "webpack-dev-server": "^1.16.2",
        "webpack-hot-middleware": "^2.14.0",
        "webworker-loader": "^0.1.0",
        "whatwg-fetch": "^2.0.1"
    },
    "jest": {
        "automock": true,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.js",
            "src/**/*.jsx"
        ],
        "moduleNameMapper": {
            "\\.scss$": "identity-obj-proxy"
        },
        "testPathDirs": [
            "src"
        ],
        "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.jsx?$",
        "unmockedModulePathPatterns": [
            "atob",
            "immutable",
            "enzyme",
            "lodash",
            "material-ui",
            "react",
            "react-redux",
            "react-router",
            "react-tap-event-plugin",
            "redux",
            "redux-devtools",
            "redux-devtools-log-monitor",
            "redux-devtools-dock-monitor",
            "reselect",
            "translit",
            "translit-russian",
            "xml-reader",
            "xml-query",
            "src"
        ]
    }
}
