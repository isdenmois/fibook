{
  "name": "fibook",
  "version": "3.4.0",
  "description": "Book manager",
  "main": "server.js",
  "repository": "https://github.com/isdenmois/fibook.git",
  "author": "Denis Moiseev <isdenmois@gmail.com>",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 npm run build:prod",
    "prebuild": "npm run build:clean",
    "build": "npm run build:prod && npm run compress",
    "build:prod": "cross-env NODE_ENV=production webpack --config dev-tools/webpack/webpack.config.prod.js --color --progress",
    "build:details": "npm run build -- --display-error-details",
    "build:clean": "rimraf ./build",
    "compress": "node ./dev-tools/gzip.js",
    "start": "cross-env NODE_ENV=development node dev-tools/server.js",
    "start:prod": "cross-env NODE_ENV=production node dev-tools/server.js",
    "start:test": "cross-env NODE_ENV=test node dev-tools/server.js",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:open": "cypress open"
  },
  "dependencies": {
    "hoist-non-react-statics": "~3.3.2",
    "jszip": "~3.2.2",
    "mobx": "~5.15.4",
    "mobx-react": "~6.1.8",
    "preact": "~10.3.4",
    "preact-compat": "~3.19.0",
    "prop-types": "~15.7.2",
    "react": "~16.13.1",
    "react-addons-css-transition-group": "~15.6.2",
    "react-dom": "~16.13.1",
    "react-router-dom": "~5.1.2",
    "redbox-react": "~1.6.0",
    "translit": "~0.1.0",
    "translit-russian": "~0.0.2",
    "xml-query": "~1.5.0",
    "xml-reader": "~2.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.0",
    "@types/enzyme": "~3.10.5",
    "@types/jest": "~24.0.13",
    "@types/jszip": "~3.1.7",
    "@types/node": "~13.9.1",
    "@types/prop-types": "~15.7.3",
    "@types/react": "~16.9.23",
    "@types/react-addons-css-transition-group": "~15.0.5",
    "@types/react-dom": "~16.9.5",
    "@types/react-router-dom": "~5.1.3",
    "add-asset-html-webpack-plugin": "~3.1.3",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "~0.1.2",
    "body-parser": "~1.19.0",
    "connect-multiparty": "~2.2.0",
    "core-js": "~3.6.4",
    "cross-env": "~7.0.2",
    "css-loader": "~3.4.2",
    "cypress-cli": "~0.14.0",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "express": "~4.17.1",
    "file-loader": "~6.0.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~25.1.0",
    "json-loader": "~0.5.7",
    "lodash": "~4.17.15",
    "md5-file": "~4.0.0",
    "mini-css-extract-plugin": "~0.9.0",
    "mobx-react-devtools": "~6.1.1",
    "morgan": "~1.9.1",
    "node-zopfli": "~2.1.3",
    "optimize-css-assets-webpack-plugin": "~5.0.3",
    "postcss-import": "~12.0.1",
    "postcss-loader": "~3.0.0",
    "postcss-modules-values": "~3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-addons-test-utils": "~15.6.2",
    "react-test-renderer": "~16.13.1",
    "rimraf": "~3.0.2",
    "shelljs": "~0.8.3",
    "sqlite3": "~4.1.1",
    "strip-loader": "~0.1.2",
    "style-loader": "~1.1.3",
    "svg-inline-loader": "~0.8.2",
    "terser-webpack-plugin": "~2.3.5",
    "typescript": "~3.8.3",
    "url-loader": "~4.0.0",
    "webpack": "~4.42.0",
    "webpack-bundle-analyzer": "~3.6.1",
    "webpack-cli": "~3.3.11",
    "webpack-common-shake": "~2.1.0",
    "webpack-dev-middleware": "~3.7.2",
    "webpack-dev-server": "~3.10.3",
    "webpack-hot-middleware": "~2.25.0",
    "worker-loader": "~2.0.0"
  },
  "jest": {
    "automock": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!**/*.d.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.svg$": "<rootDir>/dev-tools/fileTransformer.js",
      "~.+\\.jsx?$": "babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/dev-tools/tsProcessor.js"
    },
    "roots": [
      "src"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.tsx?$",
    "unmockedModulePathPatterns": [
      "atob",
      "immutable",
      "enzyme",
      "lodash",
      "material-ui",
      "react",
      "react-redux",
      "react-router",
      "react-tap-event-plugin",
      "redux",
      "redux-devtools",
      "redux-devtools-log-monitor",
      "redux-devtools-dock-monitor",
      "redux-saga",
      "redux-saga/effects",
      "reselect",
      "normalizr",
      "translit",
      "translit-russian",
      "xml-reader",
      "xml-query",
      "worker-loader",
      "src",
      "<rootDir>/dev-tools/fileTransformer.js",
      "<rootDir>/dev-tools/tsProcessor.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/dev-tools/jest.setup.js"
    ]
  },
  "browserslist": "ios 12, last 2 Chrome versions",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
