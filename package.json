{
    "name": "fibook",
    "version": "2.0.0",
    "description": "Book manager",
    "main": "server.js",
    "repository": "https://github.com/isdenmois/fibook.git",
    "author": "Denis Moiseev <isdenmois@gmail.com>",
    "license": "MIT",
    "scripts": {
        "analyze:clean": "rimraf stats.json",
        "preanalyze": "npm run analyze:clean",
        "analyze": "node ./scripts/analyze.js",
        "postinstall": "npm run build:dll",
        "prebuild": "npm run build:clean",
        "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --color --progress",
        "build:details": "npm run build -- --display-error-details",
        "build:clean": "rimraf ./build",
        "build:dll": "node ./webpack/dependencies.js",
        "compress": "node ./scripts/gzip.js",
        "g": "npm run generate",
        "generate": "plop --plopfile scripts/generators/index.js",
        "start": "cross-env NODE_ENV=development node server",
        "start:prod": "cross-env NODE_ENV=production node server",
        "lint": "eslint src",
        "test": "jest"
    },
    "dependencies": {
        "hoist-non-react-statics": "~1.2.0",
        "mobx": "~3.1.11",
        "mobx-react": "~4.2.1",
        "react": "~15.5.4",
        "react-dom": "~15.5.4",
        "react-router-dom": "~4.1.1",
        "redbox-react": "~1.4.0",
        "svg-inline-react": "~1.0.3",
        "translit": "~0.0.1",
        "translit-russian": "~0.0.2",
        "xml-query": "~1.4.0",
        "xml-reader": "~2.4.3"
    },
    "devDependencies": {
        "@types/node": "~7.0.29",
        "@types/react": "~15.0.27",
        "@types/react-dom": "~15.5.0",
        "@types/react-router-dom": "~4.0.4",
        "add-asset-html-webpack-plugin": "~2.0.1",
        "babel-core": "~6.24.1",
        "babel-eslint": "~7.2.1",
        "babel-jest": "~19.0.0",
        "babel-loader": "~7.0.0",
        "babel-plugin-transform-function-bind": "~6.22.0",
        "babel-preset-es2015": "~6.24.0",
        "babel-preset-react": "~6.24.1",
        "babili-webpack-plugin": "~0.1.1",
        "body-parser": "~1.17.2",
        "connect-multiparty": "~2.0.0",
        "cross-env": "~5.0.0",
        "css-loader": "~0.28.4",
        "enzyme": "~2.8.0",
        "eslint": "~3.19.0",
        "eslint-config-airbnb": "~14.1.0",
        "eslint-import-resolver-webpack": "~0.8.1",
        "eslint-loader": "~1.7.1",
        "eslint-plugin-import": "~2.2.0",
        "eslint-plugin-jsx-a11y": "~4.0.0",
        "eslint-plugin-react": "~6.10.3",
        "express": "~4.15.2",
        "extract-text-webpack-plugin": "~2.1.0",
        "html-webpack-plugin": "~2.28.0",
        "identity-obj-proxy": "~3.0.0",
        "jest": "~19.0.2",
        "json-loader": "~0.5.4",
        "lodash": "~4.17.4",
        "md5-file": "~3.1.1",
        "mobx-react-devtools": "~4.2.13",
        "morgan": "~1.8.1",
        "node-zopfli": "~2.0.2",
        "optimize-css-assets-webpack-plugin": "~2.0.0",
        "plop": "~1.8.0",
        "postcss-import": "~10.0.0",
        "postcss-loader": "~2.0.5",
        "postcss-modules-values": "~1.2.2",
        "react-addons-test-utils": "~15.4.2",
        "rimraf": "~2.6.1",
        "shelljs": "~0.7.7",
        "sqlite3": "~3.1.8",
        "strip-loader": "~0.1.2",
        "style-loader": "~0.18.2",
        "svg-inline-loader": "~0.7.1",
        "ts-loader": "~2.1.0",
        "typescript": "~2.3.4",
        "url-loader": "~0.5.8",
        "webpack": "~2.6.1",
        "webpack-dev-server": "~2.4.5",
        "webpack-hot-middleware": "~2.18.0",
        "worker-loader": "~0.8.0"
    },
    "jest": {
        "automock": true,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.js",
            "src/**/*.jsx"
        ],
        "moduleNameMapper": {
            "\\.css$": "identity-obj-proxy"
        },
        "transform": {
            "~.+\\.svg$": "<rootDir>/scripts/fileTransformer.js",
            "~.+\\.jsx?$": "babel-jest"
        },
        "roots": [
            "src"
        ],
        "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.jsx?$",
        "unmockedModulePathPatterns": [
            "atob",
            "immutable",
            "enzyme",
            "lodash",
            "material-ui",
            "react",
            "react-redux",
            "react-router",
            "react-tap-event-plugin",
            "redux",
            "redux-devtools",
            "redux-devtools-log-monitor",
            "redux-devtools-dock-monitor",
            "redux-saga",
            "redux-saga/effects",
            "reselect",
            "normalizr",
            "translit",
            "translit-russian",
            "xml-reader",
            "xml-query",
            "src",
            "<rootDir>/scripts/fileTransformer.js"
        ]
    }
}
