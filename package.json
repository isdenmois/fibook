{
    "name": "fibook",
    "version": "3.1.3",
    "description": "Book manager",
    "main": "server.js",
    "repository": "https://github.com/isdenmois/fibook.git",
    "author": "Denis Moiseev <isdenmois@gmail.com>",
    "license": "MIT",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 npm run build:prod",
        "postinstall": "npm run build:dll",
        "prebuild": "npm run build:clean",
        "build": "npm run build:prod && npm run compress",
        "build:prod": "cross-env NODE_ENV=production webpack --config dev-tools/webpack/webpack.config.prod.coffee --color --progress",
        "build:details": "npm run build -- --display-error-details",
        "build:clean": "rimraf ./build",
        "build:dll": "coffee ./dev-tools/webpack/dependencies.coffee",
        "compress": "coffee ./dev-tools/gzip.coffee",
        "start": "cross-env NODE_ENV=development coffee dev-tools/server.coffee",
        "start:prod": "cross-env NODE_ENV=production coffee dev-tools/server.coffee",
        "start:test": "cross-env NODE_ENV=test coffee dev-tools/server.coffee",
        "test": "jest",
        "test:e2e": "cypress run",
        "test:open": "cypress open"
    },
    "dependencies": {
        "hoist-non-react-statics": "^2.0.0",
        "mobx": "^3.2.0",
        "mobx-react": "~4.2.1",
        "preact": "~8.1.0",
        "preact-compat": "~3.16.0",
        "prop-types": "~15.5.10",
        "react": "~15.6.1",
        "react-dom": "~15.6.1",
        "react-router-dom": "~4.1.1",
        "redbox-react": "~1.4.2",
        "svg-inline-react": "~1.0.3",
        "translit": "~0.0.1",
        "translit-russian": "~0.0.2",
        "xml-query": "~1.4.0",
        "xml-reader": "~2.4.3"
    },
    "devDependencies": {
        "@types/enzyme": "~2.8.1",
        "@types/jest": "~20.0.2",
        "@types/node": "^8.0.9",
        "@types/prop-types": "~15.5.1",
        "@types/react": "~15.0.35",
        "@types/react-dom": "~15.5.1",
        "@types/react-router-dom": "~4.0.6",
        "add-asset-html-webpack-plugin": "~2.0.1",
        "babel-core": "^6.25.0",
        "babel-preset-es2015": "~6.24.0",
        "babel-preset-react": "~6.24.1",
        "babili-webpack-plugin": "~0.1.1",
        "body-parser": "~1.17.2",
        "coffeescript": "~1.12.6",
        "connect-multiparty": "~2.0.0",
        "cross-env": "~5.0.1",
        "css-loader": "~0.28.4",
        "cypress-cli": "^0.13.1",
        "enzyme": "~2.9.1",
        "express": "~4.15.3",
        "extract-text-webpack-plugin": "~2.1.2",
        "file-loader": "~0.11.2",
        "html-webpack-plugin": "^2.29.0",
        "identity-obj-proxy": "~3.0.0",
        "jest": "~20.0.4",
        "json-loader": "~0.5.4",
        "lodash": "~4.17.4",
        "md5-file": "~3.1.1",
        "mobx-react-devtools": "~4.2.15",
        "morgan": "~1.8.1",
        "node-zopfli": "~2.0.2",
        "optimize-css-assets-webpack-plugin": "~2.0.0",
        "postcss-import": "~10.0.0",
        "postcss-loader": "~2.0.6",
        "postcss-modules-values": "^1.3.0",
        "react-addons-test-utils": "^15.6.0",
        "react-test-renderer": "~15.6.1",
        "rimraf": "~2.6.1",
        "shelljs": "~0.7.8",
        "sqlite3": "~3.1.8",
        "strip-loader": "~0.1.2",
        "style-loader": "~0.18.2",
        "svg-inline-loader": "~0.7.1",
        "ts-loader": "~2.2.0",
        "typescript": "^2.4.1",
        "url-loader": "~0.5.9",
        "webpack": "^3.1.0",
        "webpack-bundle-analyzer": "~2.8.2",
        "webpack-dev-middleware": "~1.11.0",
        "webpack-dev-server": "~2.5.0",
        "webpack-hot-middleware": "~2.18.0",
        "worker-loader": "~0.8.1"
    },
    "jest": {
        "automock": true,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts",
            "src/**/*.tsx",
            "!**/*.d.ts"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "moduleNameMapper": {
            "\\.css$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "\\.svg$": "<rootDir>/dev-tools/fileTransformer.js",
            "~.+\\.jsx?$": "babel-jest",
            "\\.(ts|tsx)$": "<rootDir>/dev-tools/tsProcessor.js"
        },
        "roots": [
            "src"
        ],
        "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.tsx?$",
        "unmockedModulePathPatterns": [
            "atob",
            "immutable",
            "enzyme",
            "lodash",
            "material-ui",
            "react",
            "react-redux",
            "react-router",
            "react-tap-event-plugin",
            "redux",
            "redux-devtools",
            "redux-devtools-log-monitor",
            "redux-devtools-dock-monitor",
            "redux-saga",
            "redux-saga/effects",
            "reselect",
            "normalizr",
            "translit",
            "translit-russian",
            "xml-reader",
            "xml-query",
            "worker-loader",
            "src",
            "<rootDir>/dev-tools/fileTransformer.js",
            "<rootDir>/dev-tools/tsProcessor.js"
        ]
    }
}
