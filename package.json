{
    "name": "fibook",
    "version": "3.4.0",
    "description": "Book manager",
    "main": "server.js",
    "repository": "https://github.com/isdenmois/fibook.git",
    "author": "Denis Moiseev <isdenmois@gmail.com>",
    "license": "MIT",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 npm run build:prod",
        "postinstall": "npm run build:dll",
        "prebuild": "npm run build:clean",
        "build": "npm run build:prod && npm run compress",
        "build:prod": "cross-env NODE_ENV=production webpack --config dev-tools/webpack/webpack.config.prod.coffee --color --progress",
        "build:details": "npm run build -- --display-error-details",
        "build:clean": "rimraf ./build",
        "build:dll": "coffee ./dev-tools/webpack/dependencies.coffee",
        "compress": "coffee ./dev-tools/gzip.coffee",
        "start": "cross-env NODE_ENV=development coffee dev-tools/server.coffee",
        "start:prod": "cross-env NODE_ENV=production coffee dev-tools/server.coffee",
        "start:test": "cross-env NODE_ENV=test coffee dev-tools/server.coffee",
        "test": "jest",
        "test:e2e": "cypress run",
        "test:open": "cypress open"
    },
    "dependencies": {
        "hoist-non-react-statics": "^3.3.0",
        "jszip": "^3.2.1",
        "mobx": "^5.9.4",
        "mobx-react": "~5.4.4",
        "preact": "~8.4.2",
        "preact-compat": "~3.18.5",
        "prop-types": "~15.7.2",
        "react": "~16.8.6",
        "react-addons-css-transition-group": "^15.6.2",
        "react-dom": "~16.8.6",
        "react-router-dom": "~5.0.0",
        "redbox-react": "~1.6.0",
        "translit": "~0.1.0",
        "translit-russian": "~0.0.2",
        "xml-query": "~1.5.0",
        "xml-reader": "~2.4.3"
    },
    "devDependencies": {
        "@types/enzyme": "~3.9.3",
        "@types/jest": "~24.0.13",
        "@types/jszip": "^3.1.6",
        "@types/node": "^12.0.2",
        "@types/prop-types": "~15.7.1",
        "@types/react": "~16.8.18",
        "@types/react-addons-css-transition-group": "^15.0.5",
        "@types/react-dom": "~16.8.4",
        "@types/react-router-dom": "~4.3.3",
        "add-asset-html-webpack-plugin": "~3.1.3",
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "~6.24.1",
        "babel-preset-react": "~6.24.1",
        "babili-webpack-plugin": "~0.1.2",
        "body-parser": "~1.19.0",
        "coffeescript": "~2.4.1",
        "connect-multiparty": "~2.2.0",
        "cross-env": "~5.2.0",
        "css-loader": "~2.1.1",
        "cypress-cli": "^0.14.0",
        "enzyme": "~3.9.0",
        "express": "~4.17.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "~3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "~3.0.0",
        "jest": "~24.8.0",
        "json-loader": "~0.5.7",
        "lodash": "~4.17.13",
        "md5-file": "~4.0.0",
        "mobx-react-devtools": "~6.1.1",
        "morgan": "~1.9.1",
        "node-zopfli": "~2.0.3",
        "optimize-css-assets-webpack-plugin": "~5.0.1",
        "postcss-import": "~12.0.1",
        "postcss-loader": "~3.0.0",
        "postcss-modules-values": "^3.0.0",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "~16.8.6",
        "rimraf": "~2.6.3",
        "shelljs": "~0.8.3",
        "sqlite3": "~4.0.8",
        "strip-loader": "~0.1.2",
        "style-loader": "~0.23.1",
        "svg-inline-loader": "~0.8.0",
        "terser-webpack-plugin": "^1.3.0",
        "ts-loader": "~6.0.1",
        "typescript": "^3.4.5",
        "url-loader": "~1.1.2",
        "webpack": "^4.32.2",
        "webpack-bundle-analyzer": "~3.3.2",
        "webpack-cli": "^3.3.2",
        "webpack-common-shake": "~2.1.0",
        "webpack-dev-middleware": "~3.7.0",
        "webpack-dev-server": "~3.4.1",
        "webpack-hot-middleware": "~2.25.0",
        "worker-loader": "~2.0.0"
    },
    "jest": {
        "automock": true,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts",
            "src/**/*.tsx",
            "!**/*.d.ts"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "moduleNameMapper": {
            "\\.css$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "\\.svg$": "<rootDir>/dev-tools/fileTransformer.js",
            "~.+\\.jsx?$": "babel-jest",
            "\\.(ts|tsx)$": "<rootDir>/dev-tools/tsProcessor.js"
        },
        "roots": [
            "src"
        ],
        "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.tsx?$",
        "unmockedModulePathPatterns": [
            "atob",
            "immutable",
            "enzyme",
            "lodash",
            "material-ui",
            "react",
            "react-redux",
            "react-router",
            "react-tap-event-plugin",
            "redux",
            "redux-devtools",
            "redux-devtools-log-monitor",
            "redux-devtools-dock-monitor",
            "redux-saga",
            "redux-saga/effects",
            "reselect",
            "normalizr",
            "translit",
            "translit-russian",
            "xml-reader",
            "xml-query",
            "worker-loader",
            "src",
            "<rootDir>/dev-tools/fileTransformer.js",
            "<rootDir>/dev-tools/tsProcessor.js"
        ]
    }
}
